{
  "extends": [
    "eslint:recommended",
    "next",
    "next/core-web-vitals",
    "plugin:@typescript-eslint/recommended",
    "prettier"
  ],
  "plugins": ["prettier"],
  "rules": {
    "prettier/prettier": "error",
    "endOfLine": "off",
    "no-console": "error",
    "linebreak-style": ["error", "unix"],
    "@typescript-eslint/no-unused-vars": "error"
  }
}
